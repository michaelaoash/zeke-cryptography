

def gcd(x,y):
    if x <=0 or y <=0:
        print("Only use positive values")
    else:
        if x < y:
            tmp = x
            x = y
            y = tmp
        remainder = x % y
        while remainder > 0 :
            x = y
            y = remainder
            remainder = x % y
        return y


def findModularMultiplicativeInverse(phi_N,e):
    if gcd(phi_N,e) != 1:
        print("Not relatively prime")
    else:
        phi_N // e
        R = phi_N % e

